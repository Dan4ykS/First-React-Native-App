{
  "expo": {
    "name": "First App",
    "slug": "first-rn-app",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dan4yk.first-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.dan4yk.first_app",
      "versionCode": 1,
      "permissions": ["CAMERA","WRITE_EXTERNAL_STORAGE"]
    }
  }
}
